{
  "name": "zwl-calendar",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zachlysobey/zwl-calendar.git"
  },
  "scripts": {
    "test": "karma start",
    "test:ci": "karma start --browsers PhantomJS",
    "watch": "karma start --auto-watch --no-single-run",
    "build": "npm run build:js && npm run copy:demo && npm run copy:css",
    "clean": "rm -rf build",
    "build:js": "webpack --config webpack.config.js",
    "lint": "eslint src/ test/ ./*.js",
    "copy:demo": "cp src/example-usage.html build/index.html",
    "copy:css": "cp src/style.css build/zwl-calendar.css"
  },
  "author": "Zachary Lysobey <zachary@lysobey.com> (http://zach.lysobey.com/)",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "eslint": "^1.10.3",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.19",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^0.2.2",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.23",
    "karma-webpack": "^1.7.0",
    "phantomjs-prebuilt": "^2.1.3",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  }
}
