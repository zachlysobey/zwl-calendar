    "babel-plugin-transform-es2015-block-scoping": "^6.4.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.4.3",
    "babel-plugin-transform-es2015-parameters": "^6.4.5",
    "babel-register": "^6.4.3"
  }
}
